      "options": {
        "content": "<h1>{{variable \"myAdvise\"}} - node: {{variable \"node\"}}</h1>\n<p>\n  {{#if (isArray data)}}\n<p>\n  {{#each data}}\n  {{#if (isArray result)}}\n<ul>{{#each result}}<li>{{{urlify this}}}</li>{{/each}}</ul>\n{{else if result}}\n{{#each (linesIn result '. ')}}\n{{{separateKB this}}}{{{nextLine this 150}}}\n{{/each}}\n{{/if}}\n</p>\n{{/each}}\n{{/if}}\n</p>",
        "defaultContent": "Select a Checklist above to review the details.\r\n",
        "editor": {
          "format": "auto",
          "height": 200,
          "language": "html"
        },
        "everyRow": false,
        "helpers": "function anchor(text) {\r\n  var urlRegex = /(https?:\\/\\/[^\\s]+)\\b/gim;\r\n  var result = text.replace(urlRegex, '<a style=\"color:DodgerBlue;\" href=\"$1\">$1</a>');\r\n  return result\r\n};\r\n\r\nvar KBregex = /([^\\.!?]*)(https?:\\/\\/[^\\s]+)([^\\.!?]*[\\.!?])/gi;\r\nhandlebars.registerHelper('separateKB',\r\n  context => context.replace(KBregex, function (KB) {\r\n    return new handlebars.SafeString(\"</p><p>\" + anchor(KB) + \"</p>\");\r\n  }));\r\n\r\nhandlebars.registerHelper('urlify', function (context) {\r\n  return anchor(context)\r\n});\r\n\r\nhandlebars.registerHelper('isArray', function (text) {\r\n  return text instanceof Array;\r\n});\r\n\r\nvar lineLength = 0;\r\nhandlebars.registerHelper('nextLine', function (text, value) {\r\n  lineLength += text.length;\r\n  if (lineLength >= value) {\r\n    lineLength = 0;\r\n    return \"<br/>\";\r\n  }\r\n});\r\n\r\nhandlebars.registerHelper('testSplit', function (text, chr) {\r\n  if (text.toLowerCase().indexOf(\"copyright\") != -1) {\r\n    return text.split(chr);\r\n  }\r\n  return text;\r\n});\r\n\r\nhandlebars.registerHelper('linesIn', function (text, chr) {\r\n  lines = text.split(chr);\r\n  output = lines.map((line, index, { length }) => {\r\n    if (index + 1 != length) {\r\n      return line + chr\r\n    } else {\r\n      return line\r\n    };\r\n  });\r\n  return output;\r\n});"
